        {{#mdc-grid-list as |list|}}
          {{#list.tiles as |tiles|}}
            {{#tiles.tile as |tile|}}
              {{#tile.primary as |primary|}}
                {{primary.primary-content tagName="div" class="my-tile-image"}}
              {{/tile.primary}}
