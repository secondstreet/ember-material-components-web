    {{#freestyle-section name="Floating Action Buttons" as |section|}}
      {{#freestyle-usage "fab-bind" title="bind actions"}}

        {{#mdc-fab onclick=(action "alert" "Stop clicked") as |fab|}}
          <i class="material-icons {{iconClass}}">stop</i>
        {{/mdc-fab}}
      {{/freestyle-usage}}
      {{#freestyle-usage "fab-mini" title="Mini"}}
        {{#mdc-fab mini=true as |fab|}}
          <i class="material-icons {{iconClass}}">replay</i>
        {{/mdc-fab}}
      {{/freestyle-usage}}
      {{#freestyle-usage "fab-extended" title="Extended"}}
        {{#mdc-fab extended=true as |fab|}}
          <i class="material-icons {{iconClass}}">add</i>
          {{#fab.label}}Create{{/fab.label}}
        {{/mdc-fab}}
      {{/freestyle-usage}}
      {{#freestyle-usage "fab-extended" title="Extended (Text First)"}}
        {{#mdc-fab extended=true as |fab|}}
          {{#fab.label}}Create{{/fab.label}}
          <i class="material-icons {{iconClass}}">add</i>
        {{/mdc-fab}}
      {{/freestyle-usage}}
      {{#freestyle-usage "fab-extended" title="Extended (Without Icon)"}}
        {{#mdc-fab extended=true as |fab|}}
          {{#fab.label}}Create{{/fab.label}}
        {{/mdc-fab}}
      {{/freestyle-usage}}
      {{#freestyle-usage "fab-exited" title="Animated Exit"}}
        {{#mdc-fab exited=isFabExited as |fab|}}
          <i class="material-icons {{iconClass}}">add</i>
        {{/mdc-fab}}
        <button onclick={{action (mut isFabExited (not isFabExited))}}>{{if isFabExited "Un-exit" "Exit"}}</button>
      {{/freestyle-usage}}
    {{/freestyle-section}}
    {{#freestyle-section name="Checkboxes" as |section|}}
      {{#freestyle-usage "cbx-attributes" title="Attributes"}}
        <label>
          {{mdc-checkbox checked=isCheckboxOneChecked onchange=(action (mut isCheckboxOneChecked))}}
          Checkbox
        </label>
        <label>
          {{mdc-checkbox indeterminate=true disabled=true onchange=(action "alert" "Should not see this.")}}
          Disabled Indeterminate Checkbox
        </label>
        {{mdc-checkbox input-id="checkbox-for" checked=isCheckboxTwoChecked onchange=(action (mut isCheckboxTwoChecked))}}
        <label for="checkbox-for">Disassociated Label Checkbox</label>

      {{/freestyle-usage}}
      {{#freestyle-usage "cbx-bubbling" title="Action Bubbling/Event Propagation"}}
        <div class="test-action-bubbling" {{action "alert" "Stop the bubbling"}}>
          {{mdc-checkbox input-id="checkbox-for" checked=isCheckboxThreeChecked onchange=(action (mut isCheckboxThreeChecked)) bubbles=false}}
          <div>Checkbox event propagation is disableable: click outside of the checkbox to test</div>
        </div>
      {{/freestyle-usage}}
      {{#freestyle-usage "cbx-bindings" title="bindings"}}
        {{mdc-checkbox checked=isCheckBoxFourChecked}}
        <button {{action (toggle "isCheckBoxFourChecked" this)}}>Press to Toggle Checkbox</button>
      {{/freestyle-usage}}
    {{/freestyle-section}}
    {{#freestyle-section name="Radios" as |section|}}
      {{#freestyle-usage "radio" title="radios"}}
        <label>
          {{mdc-radio checked=isRadioOneChecked onchange=(action (mut isRadioOneChecked))}}
          Radio
        </label>
        <label>
          {{mdc-radio disabled=true onchange=(action "alert" "Should not see this.")}}
          Disabled Radio
        </label>
        {{mdc-radio input-id="radio-for" checked=isRadioTwoChecked onchange=(action (mut isRadioTwoChecked))}}
        <label for="radio-for">Disassociated Label Radio</label>
      {{/freestyle-usage}}
      {{#freestyle-usage "radio-group" title="radio Group"}}
        <fieldset>
          Radio group with shared <code>name</code> attribute:
          <label>
            {{mdc-radio name="radio-three" checked=isRadioThreeAChecked onchange=(action (mut isRadioThreeAChecked))}} A
          </label>
          <label>
            {{mdc-radio name="radio-three" checked=isRadioThreeBChecked onchange=(action (mut isRadioThreeBChecked))}} B
          </label>
          <label>
            {{mdc-radio name="radio-three" checked=isRadioThreeCChecked onchange=(action (mut isRadioThreeCChecked))}} C
          </label>
        </fieldset>

      {{/freestyle-usage}}
      {{#freestyle-usage "radio-binging" title="bindings"}}
        <div class="test-action-bubbling" {{action "alert" "Stop the bubbling"}}>
          {{mdc-radio checked=isRadioFourChecked onchange=(action (mut isRadioFourChecked)) bubbles=false}}
          <div>Radio button event propagation is disableable: click outside of the radio button to test</div>
        </div>
        {{mdc-radio checked=isRadioFiveChecked}}
        <button {{action (toggle "isRadioFiveChecked" this)}}>Press to Toggle Radio</button>
      {{/freestyle-usage}}
